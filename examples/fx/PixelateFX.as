package {	import flash.display.Sprite;	import flash.utils.getDefinitionByName;		import com.flashdynamix.motion.TweensyGroup;	import com.flashdynamix.motion.effects.PixelateEffect;	import com.flashdynamix.motion.layers.BitmapLayer;		import fl.motion.easing.Linear;		/**	 * @author shanem	 */	public class PixelateFX extends Sprite {		private var layer : BitmapLayer;		private var tween : TweensyGroup;		public function PixelateFX() {			layer = new BitmapLayer(375, 400);			tween = new TweensyGroup(false, true);						var Robot : Class = getDefinitionByName("Robot") as Class;						var pe : PixelateEffect = new PixelateEffect();						layer.draw(new Robot(0, 0));			layer.add(pe);						addChild(layer);						tween.to(pe, {amount:1}, 2, Linear.easeNone);		}	}}